{
  "name": "locomotion",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "build-android": "cd android && ./gradlew app:assembleRelease && cd .. && today=`date '+%Y_%m_%d(%H_%M_%S)'`; mv ./android/app/build/outputs/apk/release/app-release.apk ./locomotion-release-RN-$today.apk",
    "clean-android": "cd android  && ./gradlew clean",
    "postinstall": "npx jetify",
    "symlink": "FAST_REFRESH=false rm -rf ./node_modules/@autofleet/locomotion-sdk/src && cp -r ../../../locomotion-sdk/src ./node_modules/@autofleet/locomotion-sdk/src && ./node_modules/react-native/cli.js start",
    "dev": "npx nodemon --exec 'npm run symlink' --watch ../../../locomotion-sdk/src -e js,ts,tsx,jsx --delay 2.5"
  },
  "dependencies": {
    "@autofleet/locomotion-sdk": "^1.7.10",
    "@mapbox/polyline": "^1.1.0",
    "@react-native-async-storage/async-storage": "^1.17.3",
    "@react-native-community/datetimepicker": "^6.1.3",
    "@react-native-community/geolocation": "^2.0.2",
    "@react-native-firebase/app": "^14.9.1",
    "@react-native-firebase/crashlytics": "^14.9.1",
    "@react-native-picker/picker": "^2.4.1",
    "@stripe/stripe-react-native": "^0.8.0",
    "jwt-decode": "^3.1.2",
    "lottie-ios": "^3.2.3",
    "lottie-react-native": "^5.0.1",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-config": "^1.4.5",
    "react-native-device-info": "^8.7.1",
    "react-native-fast-image": "^8.5.11",
    "react-native-gesture-handler": "^2.4.2",
    "react-native-image-picker": "^4.8.2",
    "react-native-image-resizer": "^1.4.5",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-location": "^2.5.0",
    "react-native-maps": "^0.31.1",
    "react-native-mixpanel": "^1.2.5",
    "react-native-modal": "^13.0.1",
    "react-native-onesignal": "^4.3.9",
    "react-native-picker-select": "^8.0.4",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.2.5",
    "react-native-screens": "^3.13.1",
    "react-native-svg": "^12.3.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-webview": "^11.18.2",
    "unstated-next": "^1.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^27.5.1",
    "@types/react-native": "^0.67.7",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jetifier": "^2.0.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.6.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
