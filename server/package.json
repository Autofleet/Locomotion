{
    "name": "locomotion-server",
    "version": "2.1.7",
    "description": "",
    "main": "index.js",
    "scripts": {
        "build": "./node_modules/.bin/tsc",
        "coverage": "jest --coverage --forceExit --runInBand && rm -rf ./coverage",
        "test": "jest --forceExit",
        "test-auto": "jest --watch",
        "lint": "./node_modules/.bin/eslint .",
        "dev": "nodemon index.js --ignore web-ui",
        "dev:webui": "nodemon --ignore web-ui/build/ --exec \"NODE_ENV=development SKIP_PREFLIGHT_CHECK=true npm run build --prefix ./web-ui/ && node ./index.js\"",
        "install-webui": "SKIP_PREFLIGHT_CHECK=true npm install --unsafe-perm --prefix ./src/web-ui/ && SKIP_PREFLIGHT_CHECK=true npm rebuild node-sass --prefix ./src/web-ui/ && SKIP_PREFLIGHT_CHECK=true npm run build --prefix ./src/web-ui/",
        "start": "npm run init && node index.js",
        "migrate": "node_modules/.bin/sequelize db:migrate",
        "init": "bash ./scripts/init.sh",
        "clean": "rm -rf ./node_modules/ ./web-ui/node_modules/ ./web-ui/build/"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "ts-node src/index.ts",
        "ext": "ts,js"
    },
    "author": "AutoFleet",
    "license": "ISC",
    "dependencies": {
        "@google-cloud/storage": "^3.3.0",
        "@turf/turf": "^5.1.6",
        "axios": "^0.19.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^24.9.0",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.0",
        "cloudinary": "^1.14.0",
        "compression": "^1.7.4",
        "debug": "^4.1.1",
        "dotenv": "^5.0.0",
        "express": "^4.16.2",
        "git-last-commit": "^0.3.0",
        "helmet": "^3.21.2",
        "i18next": "^19.3.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.14",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nexmo": "^2.4.1",
        "nock": "^10.0.6",
        "onesignal-node": "^3.0.0",
        "pg": "^6.4.1",
        "react-select": "^3.1.0",
        "reflect-metadata": "^0.1.13",
        "regenerator-runtime": "^0.13.3",
        "sequelize": "^6.6.2",
        "sequelize-cli": "^3.2.0",
        "sequelize-replace-enum-postgres": "^1.5.0",
        "sequelize-typescript": "^2.1.0",
        "shortid": "^2.2.14",
        "ts-node": "^10.0.0",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@types/jest": "^26.0.23",
        "@types/node": "^15.6.1",
        "@types/validator": "^13.1.3",
        "@typescript-eslint/eslint-plugin": "^4.25.0",
        "@typescript-eslint/parser": "^4.25.0",
        "babel-eslint": "^10.1.0",
        "eslint": "^7.27.0",
        "eslint-config-airbnb": "^16.1.0",
        "eslint-plugin-import": "^2.18.0",
        "jest": "^24.9.0",
        "supertest": "^3.4.2",
        "ts-jest": "^26.5.6",
        "typescript": "^4.2.4"
    }
}
