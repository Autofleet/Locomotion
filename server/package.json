{
    "name": "locomotion-server",
    "version": "2.1.7",
    "description": "",
    "main": "index.js",
    "scripts": {
        "coverage": "jest --coverage --forceExit --runInBand && rm -rf ./coverage",
        "test": "jest --forceExit --runInBand",
        "test-auto": "jest --watch",
        "lint": "./node_modules/.bin/eslint --resolve-plugins-relative-to=. ./src",
        "dev": "nodemon",
        "dev:webui": "nodemon --ignore web-ui/build/ --exec \"NODE_ENV=development SKIP_PREFLIGHT_CHECK=true npm run build --prefix ./web-ui/ && node ./index.js\"",
        "install-webui": "SKIP_PREFLIGHT_CHECK=true npm install --unsafe-perm --prefix ./web-ui/ && SKIP_PREFLIGHT_CHECK=true npm rebuild node-sass --prefix ./web-ui/ && SKIP_PREFLIGHT_CHECK=true npm run build --prefix ./web-ui/",
        "start": "npm run init && node index.js",
        "migrate": "node_modules/.bin/sequelize db:migrate",
        "init": "bash ./scripts/init.sh",
        "clean": "rm -rf ./node_modules/ ./web-ui/node_modules/ ./web-ui/build/",
        "build": "echo build"
    },
    "engines": {
        "node": ">=10.0.0"
    },
    "nodemonConfig": {
        "ignore": [
            "**/*.test.ts",
            "**/*.spec.ts",
            ".git",
            "web-ui",
            "node_modules"
        ],
        "watch": [
            "src"
        ],
        "exec": "ts-node src/index.js",
        "ext": "ts,js"
    },
    "author": "AutoFleet",
    "license": "ISC",
    "dependencies": {
        "@google-cloud/storage": "^5.19.2",
        "@turf/turf": "^5.1.6",
        "axios": "^0.19.0",
        "babel-core": "^6.26.3",
        "bluebird": "^3.5.5",
        "body-parser": "^1.19.0",
        "chalk": "^4.1.0",
        "cloudinary": "^1.14.0",
        "compression": "^1.7.4",
        "debug": "^4.1.1",
        "dotenv": "^5.0.0",
        "express": "^4.16.2",
        "git-last-commit": "^0.3.0",
        "helmet": "^3.21.2",
        "i18next": "^19.3.2",
        "jsonwebtoken": "^8.5.1",
        "lodash": "^4.17.14",
        "moment": "^2.24.0",
        "morgan": "^1.9.1",
        "multer": "^1.4.2",
        "nexmo": "^2.4.1",
        "nock": "^10.0.6",
        "onesignal-node": "^3.0.0",
        "pg": "^8.7.3",
        "react-select": "^3.1.0",
        "regenerator-runtime": "^0.13.3",
        "sequelize": "^6.19.0",
        "sequelize-cli": "^6.4.1",
        "reflect-metadata": "^0.1.13",
        "sequelize-replace-enum-postgres": "^1.5.0",
        "sequelize-typescript": "^2.1.3",
        "shortid": "^2.2.14",
        "ts-node": "^10.7.0",
        "typescript": "^4.6.3",
        "uuid": "^8.3.2",
        "winston": "^3.1.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.19.0",
        "@typescript-eslint/parser": "^5.19.0",
        "babel-eslint": "^10.1.0",
        "babel-jest": "^27.5.1",
        "eslint": "^8.13.0",
        "eslint-config-airbnb": "^18.2.1",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "jest": "^27.5.1",
        "nodemon": "^2.0.15",
        "supertest": "^3.4.2",
        "ts-jest": "^27.1.4",
        "typescript-eslint": "^0.0.1-alpha.0"
    }
}
